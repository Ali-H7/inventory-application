<ul class="genre-list">
  <% genres.forEach((genre)=>{ %>
  <li class="genre-delete-container">
    <p><%=genre.genre_name%></p>
    <form action="/manage-genre/delete-genre/<%=genre.id%>" method="post">
      <button data-delete="<%=genre.id%>" class="genere-delete-button delete-dialog-open" type="button">
        <i class="fa-regular fa-x" aria-hidden="true"></i>
      </button>
      <%- include('delete-dialog', {id: genre.id}) %>
    </form>
  </li>
  <% }) %>
</ul>
